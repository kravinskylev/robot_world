<div class="index">
  <span class="all-robots">
    <h1>All Robots</h1>
  </span>


  <% @robots.each do |robot| %>
      <h3>
        <span class=index-items>
          <a href="/robots/<%= robot.id %>">
            <%= robot.title %>
          </a>
          <a href="/robots/<%= robot.id %>/edit"><img class="edit" src='/edit26.png'></a>
        </span>
      </h3>
  <% end %>

</div>

<div class="front-page">
  <div id="footer">
    <a href="https://github.com/kravinskylev/robot_world">Check it out on GitHub</a>
  </div>
</div>
